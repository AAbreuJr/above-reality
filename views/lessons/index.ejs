<%- include('../partials/header') %>


<div class="card bg-dark" id='lesson-card'>
    <div class="card-body">
        <div class='container'>
            <div id="vid">
                <iframe width="900" height="500" src="https://www.youtube.com/embed/ivkyF5GuYto" frameborder="0" allowfullscreen></iframe>
            </div>
        </div>
        <div id='lessonBtns'>
            <button id= 'prevVid' type="submit" class="btn btn-primary" onclick="PrevVid()">Previous Video</button>
            <button id='completed' type="submit" class="btn btn-primary ml-2" onclick="markComplete()" >Mark As Complete</button>
            <button id='nextVid' type="submit" class="btn btn-primary ml-3" onclick="PlayVid()">Next Video</button>
        </div>
        <div class="progress">
            <div id="myBar" class="progress-bar" role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100"></div>
        </div>
    </div>
</div>

<%- include('../partials/footer') %>